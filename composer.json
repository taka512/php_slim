{
    "name": "taka512/php_slim",
    "description": "A Slim Framework sample application for rapid development",
    "keywords": ["microframework", "slim", "rest", "router", "psr7"],
    "homepage": "http://taka512.net",
    "license": "MIT",
    "authors": [
        {
            "name": "takahashi",
            "email": "kapu512@gmail.com",
            "homepage": "http://taka512.net"
        }
    ],
    "require": {
        "slim/slim": "3.9.2",
        "slim/twig-view": "2.3.0",
        "pimple/pimple": "v3.2.3",
        "illuminate/database": "v5.5.28",
        "zendframework/zend-form": "2.11.0",
        "zendframework/zend-servicemanager": "3.3.1",
        "zendframework/zend-validator": "2.10.1",
        "zendframework/zend-math": "3.0.0",
        "zendframework/zend-session": "2.8.3",
        "zendframework/zend-authentication": "^2.5",
        "zendframework/zend-permissions-acl": "^2.6",
        "zendframework/zend-permissions-rbac": "^2.6",
        "monolog/monolog": "1.23.0",
        "davedevelopment/phpmig": "v1.5.0",
        "vlucas/phpdotenv": "v2.5.0",
        "fabpot/goutte": "v3.2.3",
        "facebook/webdriver": "1.6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "6.4.4",
        "phpunit/dbunit": "3.0.3",
        "phpstan/phpstan": "0.10.5",
        "friendsofphp/php-cs-fixer": "v2.13.1",
        "symfony/panther": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Taka512\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Taka512\\Test\\": "tests/"
        }
    },
    "config": {
        "process-timeout" : 0
    },
    "scripts": {
        "test": "phpunit"
    }

}
