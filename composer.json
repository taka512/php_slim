{
    "name": "slim/slim-skeleton",
    "description": "A Slim Framework skeleton application for rapid development",
    "keywords": ["microframework", "rest", "router", "psr7"],
    "homepage": "http://github.com/slimphp/Slim-Skeleton",
    "license": "MIT",
    "authors": [
        {
            "name": "Josh Lockhart",
            "email": "info@joshlockhart.com",
            "homepage": "http://www.joshlockhart.com/"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "slim/slim": "3.9.2",
        "slim/twig-view": "2.3.0",
        "pimple/pimple": "v3.2.3",
        "illuminate/database": "v5.5.28",
        "zendframework/zend-form": "2.11.0",
        "zendframework/zend-servicemanager": "3.3.1",
        "zendframework/zend-validator": "2.10.1",
        "zendframework/zend-math": "3.0.0",
        "zendframework/zend-session": "2.8.3",
        "monolog/monolog": "1.23.0",
        "davedevelopment/phpmig": "v1.5.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8 < 6.0"
    },
    "autoload": {
        "psr-4": {
            "Taka512\\": "src/Taka512/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Taka512\\Test\\": "tests/"
        }
    },
    "config": {
        "process-timeout" : 0
    },
    "scripts": {
        "test": "phpunit"
    }

}
