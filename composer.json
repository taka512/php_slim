{
    "name": "taka512/php_slim",
    "description": "A Slim Framework sample application for development",
    "keywords": ["slim"],
    "homepage": "http://taka512.net",
    "license": "MIT",
    "authors": [
        {
            "name": "taka512",
            "email": "kapu512@gmail.com",
            "homepage": "http://taka512.net"
        }
    ],
    "require": {
        "davedevelopment/phpmig": "v1.*",
        "doctrine/annotations": "1.*",
        "fabpot/goutte": "v4.*",
        "illuminate/database": "v8.*",
        "laminas/laminas-authentication": "2.8.*",
        "laminas/laminas-form": "3.0.*",
        "laminas/laminas-math": "3.3.*",
        "laminas/laminas-session": "2.11.*",
        "laminas/laminas-servicemanager": "3.*",
        "laminas/laminas-validator": "2.*",
        "monolog/monolog": "2.*",
        "pagerfanta/pagerfanta": "v3.*",
        "php-di/php-di": "6.*",
        "php-di/slim-bridge": "3.*",
        "php-webdriver/webdriver": "1.*",
        "pimple/pimple": "^3.0",
        "slim/slim": "4.8.*",
        "slim/twig-view": "3.*",
        "slim/psr7": "1.*",
        "symfony/console": "v5.*",
        "symfony/http-client": "v5.*",
        "symfony/panther": "v1.*",
        "symfony/var-dumper": "v5.*",
        "symfony/yaml": "v5.*",
        "twig/twig": "v3.*",
        "vlucas/phpdotenv": "v5.*",
        "zircote/swagger-php": "3.*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "v3.1.0",
        "nelmio/alice": "v3.8.2",
        "phpstan/phpstan": "0.12.96",
        "phpunit/phpunit": "9.5.8"
    },
    "autoload": {
        "psr-4": {
            "Taka512\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Taka512\\Test\\": "tests/"
        }
    },
    "config": {
        "process-timeout" : 0
    },
    "scripts": {
        "test": "phpunit"
    }

}
